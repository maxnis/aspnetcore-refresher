@model StocksApp.Models.StockTrade

@{
 ViewBag.Title = "Home";
}

<div class="box trading-panel">

 <div class="flex trading-panel-flex">

  <div id="stock-price-panel">

   <div class="stock">
    <h1 class="stock-title">@Model.StockSymbol</h1>

    <div>@Model.StockName</div>
    <div class="stock-price">$@Model.Price</div>
    <div>@Model.Quantity</div>
   </div>

  </div>
 </div>
</div>